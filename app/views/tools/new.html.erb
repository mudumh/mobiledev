<%= form_for @tool do |f|%>
	<div class="form-group">
	<%= f.label :title %>
	<%= f.text_field :title%>
	</div>
	
	<div class="form-group">
	<%= f.label :url %>
	<%= f.text_field :url%>
	</div>

	<div class="form-group">
	<%= f.label :desc %>
	<%= f.text_area :desc%>
	</div>

	<div class="form-group">
  <%= f.submit%>
	</div>
	
<% end%>